{
  "name": "fernando-tecnico-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:full": "npm run optimize:images && npm run update:image-paths && npm run update:colors && npm run generate:images && next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
    "generate:images": "node scripts/generate-images.js",
    "optimize:images": "node scripts/optimize-images.js",
    "update:image-paths": "node scripts/update-image-paths.js",
    "update:colors": "node scripts/update-colors.js",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.86.0",
    "aos": "^2.3.4",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/aos": "^3.0.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cypress": "^15.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "sharp": "^0.34.3",
    "start-server-and-test": "^2.0.13",
    "typescript": "^5"
  }
}
